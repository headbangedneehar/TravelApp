<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.db.app.travelersapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/homeActivity" />
        <activity
            android:name=".SelectActivity"
            android:label="@string/selectActivity" />
        <activity
            android:name=".EmergencyActivity"
            android:label="@string/Emergenceinfo" >
        </activity>
        <activity
            android:name=".ResultActivity"
            android:label="@string/title_activity_result" >
        </activity>
        <activity
            android:name=".DestinationlistActivity"
            android:label="@string/title_activity_result" >
        </activity>
        <activity
            android:name=".DescriptionActivity"
            android:label="@string/title_activity_description" >
        </activity>
        <activity
            android:name=".travelInfoActivity"
            android:label="@string/title_activity_travel_info" >
        </activity>
        <activity
            android:name=".demoActivity"
            android:label="@string/title_activity_demo" >
        </activity>
        <activity android:name="org.achartengine.GraphicalActivity" />
    </application>

</manifest>

<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.db.app.travelersapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.db.app.travelersapp.WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.db.app.travelersapp.HomeActivity"
            android:label="@string/homeActivity" />
        <activity
            android:name="com.db.app.travelersapp.SelectActivity"
            android:label="@string/selectActivity" />
        <activity
            android:name="com.db.app.travelersapp.EmergencyActivity"
            android:label="@string/Emergenceinfo" >
        </activity>
        <activity
            android:name="com.db.app.travelersapp.ResultActivity"
            android:label="@string/title_activity_result" >
        </activity>
        <activity
            android:name="com.db.app.travelersapp.DestinationlistActivity"
            android:label="@string/title_activity_result" >
        </activity>
        <activity
            android:name="com.db.app.travelersapp.DescriptionActivity"
            android:label="@string/title_activity_description" >
        </activity>
        <activity
            android:name="com.db.app.travelersapp.travelInfoActivity"
            android:label="@string/title_activity_travel_info" >
        </activity>
        <activity
            android:name="com.db.app.travelersapp.demoActivity"
            android:label="@string/title_activity_demo" >
